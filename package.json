{
  "name": "moov-pwa",
  "version": "0.0.0",
  "main": "scripts/moov_main.js",
  "dependencies": {
    "async": "^2.1.4",
    "buffer": "^5.0.6",
    "concurrently": "^3.5.1",
    "gulp-watch": "^5.0.0",
    "moov_cache": "1.0.4",
    "moov_perf": "1.2.5",
    "moov_rewriter": "2.2.1",
    "moov_stdlib": "2.6.1",
    "path-parser": "^3.0.1",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "moov_builder": "https://npm.moovweb.net/moov_builder/-/moov_builder-2.4.11.tgz"
  },
  "scripts": {
    "postinstall": "/bin/bash -c \"pushd pwa; yarn install; popd;\"",
    "moov": "sudo moov server -p 3030",
    "start": "concurrently \"npm run moov\" \"npm run watch\"",
    "watch": "pushd pwa && BROWSER=none yarn start",
    "build": "yarn run clean && pushd pwa && yarn run build:dev",
    "clean": "rm -rf pwa/build pwa/dist",
    "deploy:stage": "npm run build && moov deploy pepboys-pwa/stage -ignore-live",
    "deploy:nginx": "ln -sf $(pwd)/nginx.development.conf /usr/local/etc/nginx/servers/pepboys-pwa.conf && ln -sf $(pwd)/build/assets/pwa /usr/local/var/www/pepboys-pwa && sudo brew services restart nginx"
  },
  "pre-commit": {
    "run": [
      "jshint"
    ],
    "silent": true
  },
  "author": "Moovweb",
  "license": "MIT"
}
