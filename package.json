  "scripts": {
    "bundle": "node --harmony ./node_modules/.bin/moov_builder",
    "link": "f() { ln -sf moov_config-$@.json moov_config.json; }; f",
    "start": "sudo yarn run link -- local && yarn run clean && concurrently \"yarn react-dev-server\" \"npm run moov\"",
    "deploy": "yarn build && /bin/bash -c \"g() { yarn run link -- $@ && moov deploy --ignore-live --deploy-id=\"`git rev-parse HEAD`\" --notes=\"`git log -1 --pretty=format:%s`\" moovweb/moov-pwa-$@ && yarn run link -- local; }; g\"",
    "moov": "sudo moov server -p 80 --ssl-port 443 --verify-ssl-cert=false",
    "build:server": "webpack --config config/webpack/webpack.config.prod.server.js",
    "build:server:dev": "pushd pwa && webpack --config config/webpack/webpack.config.dev.server.js && popd",
    "build:client": "webpack --config config/webpack/webpack.config.prod.client.js",
    "build": "yarn run clean && yarn build:client && yarn build:server",
    "clean": "rm -rf pwa/build pwa/dist build/assets/* scripts/build/* && mkdir -p build/assets/pwa",
    "chrome": "open -a \"Google Chrome\" --args --ignore-certificate-errors",
    "react-dev-server": "node node_modules/moov-react-dev-server --server=config/webpack/webpack.config.dev.server --client=config/webpack/webpack.config.dev.client"
  },
  "author": "Moovweb",
