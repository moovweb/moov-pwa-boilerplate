  "name": "moov-pwa",
  "version": "0.1.0",
  "main": "scripts/moov_main.js",
  "dependencies": {
    "async": "^2.1.4",
    "buffer": "^5.0.6",
    "color": "^3.0.0",
    "concurrently": "^3.5.1",
    "gulp-watch": "^5.0.0",
    "history": "^4.7.2",
    "jss": "^9.8.0",
    "jss-global": "^3.0.0",
    "jss-nested": "^6.0.1",
    "material-ui": "^1.0.0-beta.27",
    "material-ui-icons": "^1.0.0-beta.17",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mobx-state-tree": "^1.3.1",
    "moov_cache": "1.0.4",
    "moov_perf": "1.2.5",
    "moov_rewriter": "2.2.1",
    "moov_stdlib": "2.6.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-jss": "^8.3.3",
    "react-router-dom": "^4.2.2",
    "react-swipeable-views": "^0.12.13",
    "request": "^2.81.0",
    "route-parser": "^0.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "^3.9.1",
    "mocha": "^5.0.1",
    "moov_builder": "https://npm.moovweb.net/moov_builder/-/moov_builder-2.4.11.tgz",
    "react-app-rewire-mobx": "^1.0.7",
    "react-app-rewired": "^1.4.0",
    "react-scripts": "1.0.17",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0",
    "webpack-node-externals": "^1.6.0"
  },
  "scripts": {
    "postinstall": "/bin/bash -c \"pushd pwa; yarn install; popd;\"",
    "test": "mocha",
    "moov": "sudo moov server -p 3030 --ssl-port 3031",
    "start": "concurrently \"npm run moov\" \"npm run watch\"",
    "prod": "sudo moov server -p 80 --ssl-port 443",
    "watch": "pushd pwa && BROWSER=none react-app-rewired start",
    "build:index": "node tasks/buildIndex.js",
    "build:ssr": "pushd pwa && NODE_ENV=production webpack --config ./webpack/webpack.config.dev-ssr.js && popd && mv build/assets/pwa/SSR.js scripts/build && yarn run build:index",
    "build": "yarn run clean && pushd pwa && react-app-rewired build && mv build/* ../build/assets/pwa && popd && yarn run build:index && yarn run build:ssr",
    "clean": "rm -rf pwa/build pwa/dist build/assets/pwa scripts/build/* && mkdir -p build/assets/pwa",
    "deploy": "npm run build && moov deploy pepboys-pwa-demo/pepboys-pwa-demo -ignore-live",
    "deploy:nginx": "ln -sf $(pwd)/nginx.development.conf /usr/local/etc/nginx/servers/pepboys-pwa.conf && ln -sf $(pwd)/build/assets/pwa /usr/local/var/www/pepboys-pwa && sudo brew services restart nginx",
    "test:pwa": "pushd pwa && react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "server": "node pwa/server/server.js"
  },
  "pre-commit": {
    "run": [
      "jshint"
    ],
    "silent": true
  },
  "author": "Moovweb",
  "license": "MIT"
}
